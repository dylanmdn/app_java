                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <TextField fx:id="soldeField" disable="true" prefHeight="33.0" prefWidth="251.0" GridPane.columnIndex="1" />
            <TextField fx:id="limiteRetraitField" prefHeight="33.0" prefWidth="251.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button layoutX="188.0" layoutY="311.0" mnemonicParsing="false" onAction="#handleSaveCompte" prefHeight="33.0" prefWidth="110.0" text="Enregister" />
      <Button layoutX="315.0" layoutY="311.0" mnemonicParsing="false" onAction="#handleCancelCompte" prefHeight="33.0" prefWidth="110.0" text="Annuler" />
   </children>
</AnchorPane>
